<meta charset="utf-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="HandheldFriendly" content="True">
<meta name="MobileOptimized" content="320">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="cleartype" content="on">
<title>{% if title %}{{ title }} | {{ data.site.title }} {% else %} {{ data.site.title }} {% endif %}</title>
<meta name="description" content="{% if description %}{{ description }} {% else %} {{ data.site.description }} {% endif %}">
{% if data.site.owner.google.verify %}<meta name="google-site-verification" content="{{ data.site.owner.google.verify }}">{% endif %}
{% if data.site.owner.bing.verify %}<meta name="msvalidate.01" content="{{ data.site.owner.bing.verify }}">{% endif %}
<link rel="canonical" href="{% if canonicalURL %}{{ canonicalURL }}{% else %}{{ data.site.url }}/{% if path %}{{ path }}/{% endif %}{% endif %}">
<link rel="shortcut icon" href="/favicon.ico">
{% if includeSocialMetaData %}{% include "partials/social-metadata.njk" %}{% endif %}
<link rel="stylesheet" type="text/css" href="/assets/styles.css">

